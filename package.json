{
  "name": "nice-helpers",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "rollup -c build/task/buildModules.js",
    "build": "npm run cleanDist && npm run buildModules && npm run buildType",
    "buildModules": "rollup -c build/task/buildModules.js",
    "buildType": "rollup -c build/task/buildType.js",
    "cleanDist": "rimraf dist",
    "add": "node build/task/add.js",
    "del": "node build/task/del.js"
  },
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    }
  },
  "keywords": [
    "helpers"
  ],
  "author": "alex",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.5.3",
    "esbuild": "^0.23.0",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.24.8"
  },
  "engines": {
    "node": ">=16"
  }
}
